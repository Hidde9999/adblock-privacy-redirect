{
    "name": "Adblocker + privacy redirect",
    "version": "1.0",
    "description": "",
    "permissions": [
        "webRequest",
        "webNavigation",
        "activeTab",
        "proxy",
        "tabs",
        "webRequestBlocking",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "storage",
        "cookies",
        "background"
    ],
"background": {
        "scripts": ["js/background/onload.js","js/background/adblocker.js", "js/background/redirect.js"]
    },
    "browser_action": {
        "default_popup": "html/index.html",
        "default_title": "settings"
    },
    "content_scripts": [
        {
            "matches": [
                "*://youtube.com/*",
                "*://www.youtube.com/*",
                "*://m.youtube.com/*"
            ],
            "css": ["css/youtube.css"],
            "js": [
                "js/content/yt.js",
                "js/content/ytAdBlocker.js",
                "js/content/ytPropagandaBlocker.js"
            ]
        },{
            "matches": [
                "*://invidious.fdn.fr/*"
            ],
            "js": [
                "js/content/yt.js"
            ]
        },{
            "matches": [
                "*://*.google.com/*",
                "*://google.com/*"
            ],
            "css": ["css/google.css"],
            "js": ["js/content/google.js"]
        },{
            "matches": [
                "*://*.nos.nl/*",
                "*://*.telegraaf.nl/*",
                "*://*.volkskrant.nl/*",
                "*://*.rtlnieuws.nl/*",
                "*://*.ad.nl/*",
                "*://*.nrc.nl/*",
                "*://*.nu.nl/*",
                "*://*.rijksoverheid.nl/*",
                "*://*.groene.nl/*"

            ],
            "css": ["css/fakeMedia.css"],
            "js": ["js/content/fakeNews.js"]
        },{
            "matches": ["<all_urls>"],
            "css": ["css/adblock.css"],
            "js": ["js/content/adblock.js"]
        },{
            "matches": ["<all_urls>"],
            "js": ["js/content/redirect.js"]
        }
    ],
    "web_accessible_resources": [
        "fonts/*.woff2"
    ],
    "manifest_version": 2
}