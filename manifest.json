{
    "name": "Adblocker + privacy redirect",
    "version": "1.0",
    "description": "",
    "permissions": [
        "webRequest",
        "activeTab",
        "proxy",
        "tabs",
        "webRequestBlocking",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "storage",
        "cookies",
        "background"
    ],
"background": {
        "scripts": ["js/background/adblocker.js", "js/background/tor.js", "js/background/removeCookies.js"]
    },
    "browser_action": {
        "default_popup": "html/index.html",
        "default_title": "rss"
    },
    "content_scripts": [
        {
            "matches": [
                "*://youtube.com/watch?*",
                "*://www.youtube.com/watch?*",
                "*://m.youtube.com/watch?*"
            ],
            "js": ["js/background/content.js"]
        }
    ],
    "web_accessible_resources": [
        "fonts/*.woff2"
    ],
    "manifest_version": 2
}