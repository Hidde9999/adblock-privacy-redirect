{
    "name": "Adblocker + privacy redirect",
    "version": "1.0",
    "description": "",
    "permissions": [
        "webRequest",
        "activeTab",
        "proxy",
        "tabs",
        "webRequestBlocking",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "storage",
        "cookies",
        "background"
    ],
"background": {
        "scripts": ["js/background/adblocker.js", "js/background/tor.js", "js/background/removeCookies.js"]
    },
    "browser_action": {
        "default_popup": "html/index.html",
        "default_title": "rss"
    },
    "content_scripts": [
        {
            "matches": [
                "*://youtube.com/*",
                "*://www.youtube.com/*",
                "*://m.youtube.com/*"
            ],
            "css": ["css/youtube.css"],
            "js": ["js/content/ytAdBlocker.js"]
        },{
            "matches": ["*://*/*"],
            "css": ["css/fakeMedia.css"],
            "js": ["js/content/fakeNews.js"]
        },{
            "matches": [
                "*://*.google.com/*",
                "*://google.com/*"
            ],
            "css": ["css/google.css"],
            "js": ["js/content/google.js"]
        },
        {
            "matches": ["<all_urls>"],
            "js": ["js/content/online.js"]
        }
    ],
    "web_accessible_resources": [
        "fonts/*.woff2"
    ],
    "manifest_version": 2
}