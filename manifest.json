{
    "name": "Adblocker + privacy redirect",
    "version": "1.0",
    "description": "",
    "permissions": ["webRequest", "activeTab", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "rss"
    },
    "content_scripts": [
        {
            "matches": [
                "*://youtube.com/watch?*",
                "*://www.youtube.com/watch?*",
                "*://m.youtube.com/watch?*"
            ],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        "fonts/*.woff2"
    ],
    "manifest_version": 2
}